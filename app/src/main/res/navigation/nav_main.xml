<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@id/nav_main"
    app:startDestination="@id/feature_meals">

    <include-dynamic
        android:id="@id/feature_meals"
        app:graphResName="nav_meals"
        app:moduleName="feature_meals" />

    <include-dynamic
        android:id="@id/feature_profile_settings"
        app:graphResName="nav_profile_settings"
        app:moduleName="profile_settings" />

    <action
        android:id="@+id/actionNavToMeals"
        app:destination="@id/feature_meals" />

    <action
        android:id="@+id/actionNavToProfileSettings"
        app:destination="@id/feature_profile_settings" />
</navigation>